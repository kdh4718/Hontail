<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="80dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imageViewListItemProfile"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/logo_image"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:id="@+id/textViewListItemProfileEmptyTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="나만의 칵테일이 없어요."
        style="@style/Font_SemiBold_22_Basic_White"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/imageViewListItemProfile"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>